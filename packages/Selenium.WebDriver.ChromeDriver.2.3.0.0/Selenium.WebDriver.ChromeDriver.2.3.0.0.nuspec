<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Selenium.WebDriver.ChromeDriver</id>
    <version>2.3.0.0</version>
    <authors>jsakamoto</authors>
